  },
  "homepage": "https://github.com/practical-computer/practical-views-js#readme",
  "devDependencies": {
    "@open-wc/testing": "^4.0.0",
    "@web/dev-server": "^0.4.6",
    "@web/test-runner": "^0.20.1",
    "jsdoc": "^4.0.4"
  }
